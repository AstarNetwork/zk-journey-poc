{
  "name": "zk-journey-poc",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run redirects:enable && remix build",
    "dev": "npm run redirects:disable && remix dev",
    "start": "netlify serve",
    "typecheck": "tsc -b",
    "redirects:enable": "shx cp .redirects public/_redirects",
    "redirects:disable": "shx rm -f public/_redirects"
  },
  "dependencies": {
    "@bandit-network/quest-widget": "^0.0.0-rc.5",
    "@netlify/functions": "^2.4.0",
    "@netlify/remix-adapter": "^2.2.0",
    "@rainbow-me/rainbowkit": "^1.3.0",
    "@remix-run/css-bundle": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "@thirdweb-dev/react": "^4.1.8",
    "@thirdweb-dev/sdk": "^4.0.15",
    "ethers": "^5.7.2",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shx": "^0.3.4",
    "uuid": "^9.0.1",
    "viem": "^1.19.9",
    "wagmi": "^1.4.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@remix-run/dev": "^2.2.0",
    "@remix-run/serve": "^2.2.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.7",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
